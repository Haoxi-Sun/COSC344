DROP TABLE emp_dep;

CREATE TABLE emp_dep(
       fname           VARCHAR2(15) NOT NULL,
       lname           VARCHAR2(15) NOT NULL,
       name  VARCHAR2(25),
       sex   CHAR,
       dob   DATE
       );
       
INSERT INTO emp_dep
SELECT fname, lname,dependent_name,dependent.sex,dependent.bdate
FROM employee, dependent
WHERE ssn = essn;

COMMIT;
SELECT*
FROM emp_dep
ORDER BY fname;
