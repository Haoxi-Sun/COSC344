INSERT INTO department VALUES
('TempDept',6,'123456789', TO_DATE('18-Jul-2002','DD-MON-YYYY'));

INSERT INTO project VALUES('TemProject',50,'Houston', 6);


SELECT pnumber
FROM project
WHERE dnum IN (
              SELECT dnumber
              FROM department
              WHERE mgrssn = '123456789'
              )
UNION
SELECT pnumber
FROM project
WHERE pnumber IN (
                 SELECT pno
                 FROM works_on
                 WHERE essn = '123456789'
                 );
